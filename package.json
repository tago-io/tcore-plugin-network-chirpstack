{
  "name": "@tago-io/tcore-plugin-network-chirpstack",
  "version": "0.0.1",
  "private": false,
  "main": "./build/index.js",
  "engines": {
    "tagocore": "<= 0.3.0"
  },
  "tagocore": {
    "name": "Chirpstack LoRaWAN",
    "short_description": "Add support for Chirpstack integration",
    "full_description": "./README.md",
    "icon": "./assets/icon.png",
    "types": [
      "service",
      "encoder",
      "action-type"
    ],
    "permissions": [
      "device",
      "action",
      "device-data"
    ]
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "rm -rf ./build; tsc"
  },
  "dependencies": {
    "@tago-io/tcore-sdk": "0.0.1-alpha.6",
    "async": "3.2.1",
    "axios": "^0.25.0",
    "body-parser": "^1.19.2",
    "express": "^4.17.2",
    "mqtt-connection": "4.1.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/express": "^4.17.13",
    "@types/ms": "^0.7.31",
    "@types/node": "16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  }
